<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane id="background-pane" fx:id="root" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainScreen">
   <Pane id="background-pane2" prefHeight="200.0" prefWidth="200.0" />
   <VBox id="content-pane" alignment="CENTER" fillWidth="false" maxHeight="-Infinity" maxWidth="-Infinity" spacing="20.0">
      <Button id="full-width-button" ellipsisString="   SP   " mnemonicParsing="false" onAction="#doSinglePlayer" prefWidth="200.0" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
         <graphic>
            <Label id="full-width-button-label" text="Single Player" textFill="#f8f7f5">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </graphic>
      </Button>
      <Button id="full-width-button" disable="true" ellipsisString="   NP   " mnemonicParsing="false" onAction="#doNetPlay" prefWidth="200.0" textAlignment="CENTER" textFill="#f8f7f5">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <graphic>
            <Label id="full-width-button-label" text="Net Play" textFill="#f8f7f5">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </graphic>
      </Button>
      <Button id="full-width-button" ellipsisString="   CL   " mnemonicParsing="false" onAction="#doCardList" prefWidth="200.0" textAlignment="CENTER" textFill="#f8f7f5">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <graphic>
            <Label id="full-width-button-label" text="Card List" textFill="#f8f7f5">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </graphic>
      </Button>
      <Button id="full-width-button" ellipsisString="   C   " mnemonicParsing="false" onAction="#doCredits" prefWidth="200.0" textAlignment="CENTER" textFill="#f8f7f5">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <graphic>
            <Label id="full-width-button-label" text="Credits" textFill="#f8f7f5">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </graphic>
      </Button>
      <Button id="full-width-button" ellipsisString="CONFIG" mnemonicParsing="false" onAction="#doConfiguration" prefWidth="200.0" textAlignment="CENTER" textFill="#f8f7f5">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <graphic>
            <Label id="full-width-button-label" text="Configuration" textFill="#f8f7f5">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </graphic>
      </Button>
      <Button id="full-width-button" ellipsisString="    D    " mnemonicParsing="false" onAction="#goToDanmaku" prefWidth="200.0" textAlignment="CENTER" textFill="#f8f7f5">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
         <graphic>
            <Label id="full-width-button-label" text="Go to Site" textFill="#f8f7f5">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </graphic>
      </Button>
   </VBox>
</StackPane>
